<?xml version="1.0"?>
<package >
  <metadata>
    <id>MVCGrid.Net</id>
    <version>1.0.0</version>
    <authors>Joe Harrison</authors>
    <owners>Joe Harrison</owners>
    <licenseUrl>https://github.com/bigjoe714/MVCGrid.Net/blob/master/LICENSE</licenseUrl>
    <projectUrl>http://mvcgrid.net</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A grid for ASP.NET MVC and Bootstrap with ajax paging and sorting. Also has filtering support, export to csv, back button support, and graceful degradation. Easily configurable. All AJAX calls are handled for you... Simply tell it how to get your data!</description>
    <copyright>Copyright 2015-2017</copyright>
	<releaseNotes>Added localization support. Fixed bug in paging display.</releaseNotes>
    <tags>mvc grid</tags>
    <dependencies>
      <dependency id="Microsoft.AspNet.Mvc" version="3.0" />
      <dependency id="WebActivatorEx" version="[2.0, 3.0)" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\MVCGrid.dll" target="lib\net40" />
    <file src="bin\Release\MVCGrid.xml" target="lib\net40" />
    <file src="..\NuGetFiles\MVCGridConfig.cs.pp" target="Tools" />
    <file src="..\NuGetFiles\web.config.transform" target="Content" />
    <file src="..\NuGetFiles\readme.txt" target="" />
    <file src="..\NuGetFiles\Views\web.config.transform" target="Content\Views" />
    <file src="..\NuGetFiles\Install.ps1" target="Tools" />
  </files>
</package>